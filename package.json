{
  "name": "benjohnsons-portfolio",
  "description": "Benjamin Johnson's website",
  "version": "1.1.0",
  "engines": {
    "node": "12.x"
  },
  "author": "Benjamin Johnson <benjamindavjohnson@gmail.com>",
  "keywords": [
    "nextjs",
    "front-end",
    "engineer",
    "portfolio"
  ],
  "license": "MIT",
  "main": "n/a",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "TZ=UTC yarn next build",
    "build:analyze": "ANALYZE=true yarn next build",
    "dev": "yarn next",
    "start": "yarn next start",
    "export": "yarn next export",
    "ts": "tsc",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "jest",
    "e2e": "yarn e2e:open",
    "e2e:open": "TZ=UTC yarn cypress open",
    "e2e:run": "TZ=UTC yarn cypress run",
    "e2e:temp": "yarn playwright test e2e --config=playwright.config.ts"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.5.0",
    "@mdx-js/loader": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "@next/bundle-analyzer": "^9.4.4",
    "@next/mdx": "^9.4.4",
    "autoprefixer": "^10.0.4",
    "clsx": "^1.1.1",
    "date-fns": "^2.14.0",
    "front-matter": "^4.0.2",
    "lodash": "^4.17.11",
    "next": "11",
    "next-mdx-enhanced": "^3.0.0",
    "next-mdx-remote": "^1.0.0",
    "preact": "^10.6.4",
    "preact-render-to-string": "^5.1.19",
    "prismjs": "^1.15.0",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-helmet": "^5.2.0",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "rehype-highlight": "^4.0.0",
    "stringify-object": "^3.3.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@playwright/test": "^1.15.0",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.41",
    "cypress": "^5.6.0",
    "dotenv": "^8.2.0",
    "gh-pages": "^2.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "next-plugin-preact": "^3.0.6",
    "postcss": "8",
    "prettier": "^1.9.2",
    "react-test-renderer": "^17.0.1",
    "react-testing-library": "^6.0.0",
    "remark-frontmatter": "^2.0.0",
    "tailwindcss": "^2.0.1",
    "tailwindcss-pseudo-elements": "^1.5.1",
    "typescript": "^4.1.3"
  }
